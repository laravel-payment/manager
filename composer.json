{
    "name": "mxp100/laravel-payment",
    "description": "Laravel payment system",
    "type": "laravel-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Yuriy Potemkin",
            "email": "ysoft2000@mail.ru"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.1.3",
        "ext-json": "*",
        "illuminate/http": "~5.7.0|~5.8.0|~5.9.0",
        "illuminate/support": "~5.7.0|~5.8.0|~5.9.0",
        "illuminate/contracts": "~5.7.0|~5.8.0|~5.9.0"
    },
    "autoload": {
        "psr-4": {
            "Mxp100\\LaravelPayment\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mxp100\\LaravelPayment\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mxp100\\LaravelPayment\\ServiceProvider"
            ],
            "aliases": {
                "Payment": "Mxp100\\LaravelPayment\\Facades\\Payment",
                "Payout": "Mxp100\\LaravelPayment\\Facades\\Payout"
            }
        }
    }
}
